{
  "scripts": {
    "ng": "ng",
    "mv": "move-cli --mkdirp",
    "start": "ng serve",
    "build": "npm run mv -- src/main/webapp/WEB-INF src/main/temp/WEB-INF && npm-run-all \"ng -- build --prod {@}\" \"mv -- src/main/temp/WEB-INF src/main/webapp/WEB-INF\" -c --",
